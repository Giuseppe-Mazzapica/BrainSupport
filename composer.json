{
    "name": "brain/support",
    "description": "Utilities for Brain https://github.com/Giuseppe-Mazzapica/Brain",
    "keywords": ["WordPress"],
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Giuseppe-Mazzapica/BrainSupport/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/BrainSupport"
    },
    "license": "GPL-2.0+",
    "require": {
        "php": ">=5.4",
        "brain/brain": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Brain\\": "src/"
        },
        "files": ["functions.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Brain\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    }
}
